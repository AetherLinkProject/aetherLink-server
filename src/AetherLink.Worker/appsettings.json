{
  "Worker": {
    "SearchTimer": 3,
    "LogBackFillBatchSize": 10,
    "BlockBackFillDepth": 100,
    "HealthCheckTimer": 50,
    "HealthCheckMaxRetryTimes": 10,
    "Chains": [
      {
        "ChainId": "AELF",
        "LatestHeight": 0
      }
    ]
  },
  "Chains": {
    "ChainInfos": {
      "AELF": {
        "BaseUrl": "http://127.0.0.1:8000",
        "OracleContractAddress": "*",
        "ConsensusContractAddress": "*"
      }
    }
  },
  "Redis": {
    "Configuration": "127.0.0.1:6379"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Warning",
        "System": "Warning",
        "Microsoft": "Warning",
        "Quartz": "Warning"
      }
    },
    "Serilog": {
      "WriteTo": [
        {
          "formatter": {
            "type": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
          }
        }
      ]
    }
  },
  "GraphQL": {
    "Configuration": "http://127.0.0.1:9001/graphql"
  },
  "Hangfire": {
    "RedisStorage": {
      "Host": "127.0.0.1:6379",
      "Prefix": "hangfire"
    },
    "WorkerCount": 1,
    "UseDashboard": true
  },
  "Network": {
    "ListenPort": 8001,
    "Index": 0,
    "Domains": []
  },
  "OracleChainInfo": {
    "ChainConfig": {
      "AELF": {
        "VRFSecret": "*",
        "TransmitterSecret": "*",
        "SignerSecret": "*",
        "DistPublicKey": []
      }
    }
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:5001"
      }
    }
  },
  "ProcessJob": {
    "RetryCount": 5
  }
}