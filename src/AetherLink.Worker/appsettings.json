{
  "Worker": {
    "SearchTimer": 3000,
    "UnconfirmedTimer": 100,
    "PollerTimer": 3000
  },
  "Chains": {
    "ChainInfos": {
      "AELF": {
        "BaseUrl": "http://127.0.0.1:8000",
        "OracleContractAddress": "*",
        "ConsensusContractAddress": "*"
      },
      "Ton": {
        "ContractAddress": "",
        "IndexerPeriod": 9,
        "OracleNodeInfoList": [],
        "IndexerProvider": ["GetBlock", "TonApi", "ChainStack"],
        "CommitProvider": ["TonCenter"],
        "Indexer":{
          "TonCenter": {
            "Url":"https://testnet.toncenter.com/api/v3/",
            "ApiKey": ""
          },
          "GetBlock": {
            "Url":"https://go.getblock.io/",
            "ApiKey": ""
          },
          "TonApi":{
            "Url": "https://testnet.tonapi.io",
            "ApiKey":""
          },
          "ChainStack":{
            "Url": "https://ton-testnet.core.chainstack.com/",
            "ApiKey":""
          }
        }
      }
    }
  },
  "Redis": {
    "Configuration": "127.0.0.1:6379"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Default": "Warning",
        "System": "Warning",
        "Microsoft": "Warning",
        "Quartz": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "Logs/log-{Date}.log",
          "retainedFileCountLimit": 15,
          "formatter": "Serilog.Formatting.Compact.RenderedCompactJsonFormatter, Serilog.Formatting.Compact"
        }
      }
    ]
  },
  "AeFinder": {
    "BaseUrl": "***"
  },
  "Hangfire": {
    "RedisStorage": {
      "Host": "127.0.0.1:6379",
      "Prefix": "hangfire"
    },
    "WorkerCount": 1,
    "UseDashboard": true
  },
  "Network": {
    "ListenPort": 8001,
    "Index": 0,
    "Domains": [""]
  },
  "OracleChainInfo": {
    "ChainConfig": {
      "AELF": {
        "VRFSecret": "*",
        "TransmitterSecret": "*",
        "SignerSecret": "*",
        "DistPublicKey": []
      },
      "Ton": {
        "TransmitterSecretKey": "*"
      }
    }
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://*:5001"
      }
    }
  },
  "ProcessJob": {
    "RetryCount": 5
  },
  "TokenPrice": {
    "BaseUrl": "http://127.0.0.1:8013"
  },
  "Metrics": {
    "Port": 19001
  }
}